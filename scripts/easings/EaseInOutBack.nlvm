package scripts.easings;

public class EaseInOutBack extends Easing implements IEasable
{
    public static final Easing Instance = new EaseInOutBack();

    public float Get(float t)
    {
        float c1 = 1.70158f;
        float c2 = c1 * 1.525f;

        return t < 0.5f 
        ? ((float)Math.pow(2 * t, 2) * ((c2 + 1) * 2 * t - c2)) / 2.f
        : ((float)Math.pow(2 * t - 2, 2) * ((c2 + 1) * (t * 2 - 2) + c2) + 2) / 2.f; 
    }
}