package scripts;

import nlvm.lang.Object;
import nlvm.lang.Float;

public final class MutableFloat extends Object
{
    private float m_Value = 0;

    public MutableFloat(){}
    public MutableFloat(float value) { m_Value = value; }
    public MutableFloat(Float value) { m_Value = value.floatValue(); }

    public void SetValue(float value) { m_Value = value; }
    public float GetValue() { return m_Value; }
}