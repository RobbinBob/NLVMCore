package scripts;
import nlvm.lang.System;
import nlvm.lang.String;

public class NullReferenceException extends Exception
{
    public static void Throw()
    {
        System.throwRuntimeException("NullReferenceException");
    }
    public static void Throw(String message)
    {
        System.throwRuntimeException("NullReferenceException | Message: " + message);
    }
}