package scripts;

public final class AudioPriority extends Object
{
    private int m_Priority = 0;
    private String m_AudioId = "";

    public AudioPriority(String name, int priority)
    {
        m_Priority = priority;
        m_AudioId = name;
    }

    public int GetPriority()
    {
        return m_Priority;
    }
    public String GetAudioName()
    {
        return m_AudioId;
    }
}