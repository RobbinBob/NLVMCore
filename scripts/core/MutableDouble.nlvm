package scripts.core;

import nlvm.lang.Double;

public final class MutableDouble extends Object
{
    private double m_Value = 0;

    public MutableDouble(){}
    public MutableDouble(double value) { m_Value = value; }
    public MutableDouble(Double value) { m_Value = value.doubleValue(); } 

    public void SetValue(double value) { m_Value = value; }
    public double GetValue() { return m_Value; }
}