package scripts;

import nlvm.lang.System;
import nlvm.lang.String;

/// <class>
/// <desc>Exception for when files have invalid formats and cannot be read.</desc>
public class InvalidFormatException extends Exception
{
    public static void Throw()
    {
        System.throwRuntimeException("InvalidFormatException");
    }
    public static void Throw(String message)
    {
        System.throwRuntimeException(CreateThrowMessage("InvalidFormatException", message));
    }
}
/// </class>